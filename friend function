 #include<iostream>
using namespace std;

class HumaBeing{

private:
	string name;
	int age;
public:
	HumaBeing(string word, int val){
		name = word;
		age = val;
	}
	friend void Display(HumaBeing Ashok);

//friend class Student;

};
void Display(HumaBeing Ashok){
	cout<<Ashok.nameendl;
}

int main()
{
	
	HumaBeing Ashok("Ashok", 28);
	Display(Ashok);
	
	
	


	


}
