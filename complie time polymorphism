// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;

class A{
  public:
  void add(int x,int y){
      cout<<x+y<<endl;
  }
  void add(int x,int y,int z){
      cout<<x+y+z<<endl;
  }
};
int main() {
    // Write C++ code here
    A a;
    a.add(10,20);
    a.add(30,40,50);
    

    return 0;
}
