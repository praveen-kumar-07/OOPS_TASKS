#include <iostream>
using namespace std;
class Friend
{
private:
	int age;
	string name;
public:
	Friend(int val,string peru){
		age=val;
		name=peru;
	}
	Friend(string peru){
		age=0;
		name=peru;
	}
	Friend(int val){
		age=val;
		name="peru ivu ra";
	}
	void Display(){
		cout<<name<<" "<<age<<endl;
	}
};
int main(){
	int x;string y;
	cin>>x>>y;
	Friend praveen(x);
	//praveen.Friend(y);
	//praveen.Friend(x);
	praveen.Display();
}
